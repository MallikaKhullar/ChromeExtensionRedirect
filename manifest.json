{
    "manifest_version": 2,
    "name": "Redirect New-Tab",
    "short_name": "Redirect",
    "description": "Redirect to your website on new-tab",
    "version": "0.0.2",
    "chrome_url_overrides": {
        "newtab": "main.html"
    },
    "permissions": ["storage", "chrome://favicon/"],
    "minimum_chrome_version": "22"
}